{
    "annotations": {
        "list": []
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "id": null,
    "links": [],
    "liveNow": false,
    "panels": [
        {
            "title": "Pipeline Overview",
            "type": "row",
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 0
            },
            "collapsed": false
        },
        {
            "title": "Job Throughput",
            "type": "stat",
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 0,
                "y": 1
            },
            "targets": [
                {
                    "expr": "sum(rate(aggregation_jobs_total[5m])) * 60",
                    "legendFormat": "Jobs/min"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "red",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 5
                            },
                            {
                                "color": "green",
                                "value": 15
                            }
                        ]
                    }
                }
            },
            "options": {
                "colorMode": "background",
                "graphMode": "area"
            }
        },
        {
            "title": "Success Rate",
            "type": "stat",
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 6,
                "y": 1
            },
            "targets": [
                {
                    "expr": "(sum(rate(aggregation_jobs_total{status=\"success\"}[5m])) / sum(rate(aggregation_jobs_total[5m]))) * 100",
                    "legendFormat": "Success %"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "min": 0,
                    "max": 100,
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "red",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 90
                            },
                            {
                                "color": "green",
                                "value": 98
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "DLQ Size",
            "type": "stat",
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 12,
                "y": 1
            },
            "targets": [
                {
                    "expr": "aggregation_dlq_size",
                    "legendFormat": "DLQ Jobs"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 10
                            },
                            {
                                "color": "red",
                                "value": 50
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "Open Circuits",
            "type": "stat",
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 18,
                "y": 1
            },
            "targets": [
                {
                    "expr": "count(aggregation_circuit_state == 1) or vector(0)",
                    "legendFormat": "Open"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "red",
                                "value": 1
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "Queue Depths",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 5
            },
            "targets": [
                {
                    "expr": "aggregation_queue_depth",
                    "legendFormat": "{{ queue }}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "custom": {
                        "fillOpacity": 20,
                        "lineWidth": 2
                    }
                }
            },
            "options": {
                "legend": {
                    "displayMode": "table",
                    "placement": "right"
                }
            }
        },
        {
            "title": "Job Duration (P95)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 5
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, rate(aggregation_job_duration_seconds_bucket[5m]))",
                    "legendFormat": "{{ queue }}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "custom": {
                        "fillOpacity": 10
                    }
                }
            }
        },
        {
            "title": "Job Throughput by Queue",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 13
            },
            "targets": [
                {
                    "expr": "sum by (queue, status) (rate(aggregation_jobs_total[5m])) * 60",
                    "legendFormat": "{{ queue }} - {{ status }}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "custom": {
                        "fillOpacity": 30
                    }
                }
            }
        },
        {
            "title": "Circuit Breaker States",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 13
            },
            "targets": [
                {
                    "expr": "aggregation_circuit_state",
                    "legendFormat": "{{ dependency }}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "mappings": [
                        {
                            "type": "value",
                            "options": {
                                "0": {
                                    "text": "Closed"
                                }
                            }
                        },
                        {
                            "type": "value",
                            "options": {
                                "1": {
                                    "text": "Open"
                                }
                            }
                        },
                        {
                            "type": "value",
                            "options": {
                                "2": {
                                    "text": "Half-Open"
                                }
                            }
                        }
                    ],
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "red",
                                "value": 1
                            },
                            {
                                "color": "yellow",
                                "value": 2
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "Dependencies",
            "type": "row",
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 21
            },
            "collapsed": false
        },
        {
            "title": "CMS API Latency (P95)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 22
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, rate(aggregation_cms_latency_seconds_bucket[5m]))",
                    "legendFormat": "{{ endpoint }}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                }
            }
        },
        {
            "title": "Storage Operations",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 22
            },
            "targets": [
                {
                    "expr": "sum by (operation, status) (rate(aggregation_storage_operations_total[5m])) * 60",
                    "legendFormat": "{{ operation }} - {{ status }}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short"
                }
            }
        },
        {
            "title": "Resources",
            "type": "row",
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 30
            },
            "collapsed": false
        },
        {
            "title": "Memory Usage",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 31
            },
            "targets": [
                {
                    "expr": "process_resident_memory_bytes / 1024 / 1024",
                    "legendFormat": "RSS (MB)"
                },
                {
                    "expr": "nodejs_heap_size_used_bytes / 1024 / 1024",
                    "legendFormat": "Heap Used (MB)"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "decmbytes"
                }
            }
        },
        {
            "title": "CPU Usage",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 31
            },
            "targets": [
                {
                    "expr": "rate(process_cpu_user_seconds_total[1m]) * 100",
                    "legendFormat": "User %"
                },
                {
                    "expr": "rate(process_cpu_system_seconds_total[1m]) * 100",
                    "legendFormat": "System %"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent"
                }
            }
        }
    ],
    "refresh": "10s",
