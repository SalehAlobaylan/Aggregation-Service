{
  "name": "ggregation-service",
  "version": "1.0.0",
  "description": "Wahb Aggregation Service - Content ingestion and processing pipeline",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "seed": "tsx src/scripts/seed-jobs.ts",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:e2e": "vitest run tests/e2e",
    "test:contract": "vitest run tests/contract",
    "test:failure": "vitest run tests/failure",
    "test:integration": "vitest run tests/integration",
    "test:load": "tsx tests/load/synthetic-producer.ts",
    "test:media": "tsx src/scripts/test-media-job.ts",
    "test:ai": "tsx src/scripts/test-ai-job.ts"
  },
  "keywords": [
    "wahb",
    "aggregation",
    "bullmq",
    "fastify"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@mozilla/readability": "^0.5.0",
    "@xenova/transformers": "^2.17.2",
    "bullmq": "^5.34.3",
    "cheerio": "^1.0.0",
    "dotenv": "^17.2.4",
    "fastify": "^5.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.1",
    "ioredis": "^5.4.2",
    "jsdom": "^25.0.1",
    "pino": "^9.6.0",
    "prom-client": "^15.1.3",
    "rss-parser": "^3.13.0",
    "uuid": "^11.0.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsdom": "^21.1.7",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^22.10.7",
    "@types/uuid": "^10.0.0",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
